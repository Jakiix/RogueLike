        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (hp &lt;= 0)
{
    game_restart();
}

if(global.Pause) 
{
    image_speed = 0;
    exit
}
else
    image_speed = 0.2
    
if (Player_obj.Nfloor == 1 and Poped == 0)
{
    x = view_xview[0] + (1920 / 2)
    y = view_yview[0] + (1080 / 2)
    Poped = 1
}

if (keyboard_check(global.key_up) &amp;&amp; keyboard_check(global.key_left))
    movespeed = 5;
else if (keyboard_check(global.key_up) &amp;&amp; keyboard_check(global.key_right))
    movespeed = 5; 
else if (keyboard_check(global.key_down) &amp;&amp; keyboard_check(global.key_left))
    movespeed = 5;
else if (keyboard_check(global.key_down) &amp;&amp; keyboard_check(global.key_right)) 
    movespeed = 5;
else
    movespeed = 6;

pos = ds_list_find_index(Player_obj.list_mob_room, Player_obj.room_id)
if (pos != -1)
{
    if (instance_exists(Random_generation) and Player_obj.room_id != Random_generation.id_boss)
    {
        alarm[3] = 50
    }
    else if (instance_exists(Random_generation2) and Player_obj.room_id != Random_generation2.id_boss)
    {
        alarm[3] = 50
    }
    else
    {
        if (place_empty(view_xview[0] + (view_wview[0] / 2), view_yview[0] + (view_hview[0] / 2)))
            instance_create(view_xview[0] + (view_wview[0] / 2), view_yview[0] + (view_hview[0] / 2), obj_pacman);
    }
    ds_list_delete(Player_obj.list_mob_room, pos);
}
if (instance_exists(obj_parent_monster))
{
        if (doorslocked != true)
        {
            doorslocked = true;
            audio_play_sound(DoorLocked, 10, false);
        } 
        with(Door_top_obj)
            image_index = 1;
        with(Door_bottom_obj)
            image_index = 1;
        with(Door_left_obj)
            image_index = 1;
        with(Door_right_obj)
            image_index = 1;
    }
    else if (!instance_exists(obj_parent_monster))
    {
        if (doorslocked == true)
        {
            doorslocked = false;
            audio_play_sound(DoorOpened, 10, false);
            with(Pikachu_obj)
            {
                instance_destroy();
            }
        }
        with(Door_top_obj)
            image_index = 0;
        with(Door_bottom_obj)
            image_index = 0; 
        with(Door_left_obj)
            image_index = 0; 
        with(Door_right_obj)
            image_index = 0;
        //random_pickup();
    }

    
if (canmove == true)
{
    if (keyboard_check(global.key_up) &amp;&amp; !place_meeting(x, y - movespeed, obj_shot)) 
    {
        movingup = true;
        movingdown = false;
        movingleft = false;
        movingright = false;
        y -= movespeed;
    }
    if (keyboard_check(global.key_down) &amp;&amp; !place_meeting(x, y + movespeed, obj_shot)) 
    {
        movingup = false;
        movingdown = true;
        movingleft = false;
        movingright = false;
        y += movespeed;
    }
    if (keyboard_check(global.key_left) &amp;&amp; !place_meeting(x - movespeed, y, obj_shot)) 
    {
        movingup = false;
        movingdown = false;
        movingleft = true;
        movingright = false;
        x -= movespeed;
    }
    if (keyboard_check(global.key_right) &amp;&amp; !place_meeting(x + movespeed, y, obj_shot)) 
    {
        movingup = false;
        movingdown = false;
        movingleft = false;
        movingright = true;
        x += movespeed;
    }
}
else {image_speed = 0;}

if (movingup == true)
{
        if (image_index &gt;= 9)
        {
            image_index = 0;
        }
}
else if (movingdown == true)
{
        if (image_index &gt;= 9)
        {
            image_index = 0;
        }
}
else if (movingright == true)
{
        if (image_index &lt; 10 || image_index &gt; 19)
        {
            image_index = 10;
        }
}
else if (movingleft == true)
{
        if (image_index &lt; 20)
        {
            image_index = 20;
        }
}

if (x == xprevious &amp;&amp; y == yprevious)
{
    image_speed = 0;
}

if (laser == false)
{
    if (keyboard_check(global.key_shot_up))
    {
        if (firing == false)
        {   
            global.dir = 90;
            firing = true;
            alarm[0] = 15;
        }  
    }
    if (keyboard_check(global.key_shot_down))
    {
        if (firing == false)
        {   
            global.dir = 270;
            firing = true;
            alarm[0] = 15;
        }  
    }
    if (keyboard_check(global.key_shot_left))
    {
        if (firing == false)
        {   
            global.dir = 180;
            firing = true;
            alarm[0] = 15;
        }  
    }
    if (keyboard_check(global.key_shot_right))
    {
        if (firing == false)
        {   
            global.dir = 0;
            firing = true;
            alarm[0] = 15;
        }  
    }
}

image_blend = c_white;

if (place_meeting(x, y, obj_parent_monster))
{
   var nearestEnemy;
   nearestEnemy = instance_nearest(x, y,obj_parent_monster)
   direction = point_direction(x, y, nearestEnemy.x, nearestEnemy.y) + 180;
   bounce = 1;
   alarm[1] = 10;
}
else if (place_meeting(x, y, obj_parent_shot))
{
    var nearest_enemy;
    nearest_enemy = instance_nearest(x, y, obj_parent_shot);
    if (nearest_enemy.enemies != false)
    {
        direction = point_direction(x, y, nearest_enemy.x, nearest_enemy.y) + 180;
        bounce = 1;
        alarm[1] = 10;
        hp -= nearest_enemy.damage;
        if (image_blend != c_red)
            image_blend = c_red;
        if (object_is_ancestor(obj_shot_boomrang, obj_parent_shot) != true)
        {
            with (nearest_enemy)
            {
                instance_destroy();
                instance_create(x, y, obj_end_ennemy_shot);
            }
        }
    }
}
if (bounce == 1)
{
    speed = 5;
    bounce = 0;
}

if (place_meeting(x, y, obj_parent_monster))
{
    x = antistickx;
    y = antisticky;
}
else if (place_meeting(x, y, Wall_parent))
{
    x = antistickx;
    y = antisticky;
}
else
{
    antistickx = x;
    antisticky = y;
}

if (pikachu == true)
{
    var nearestmob1;
    nearestmob1 = instance_nearest(x, y, obj_parent_monster)
    if (nearestmob1 != noone)
    {
        if (instance_in_view(nearestmob1, view_xview[0], view_yview[0]) == true)
        {
            if (!instance_exists(Pikachu_obj))
                instance_create(x + 100, y, Pikachu_obj);
        }
    }
}

