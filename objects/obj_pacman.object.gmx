{
    /*if (mp_grid_path(grid, path, x, y, Player_obj.x, Player_obj.y, 0))
    {
        path_start(path, 2, path_action_stop, false); 
    }*/
    move_towards_point(Player_obj.x, Player_obj.y, 3);
}

if (place_meeting(x, y, Player_obj))
    Player_obj.hp--

if (place_meeting(x, y, obj_parent_shot))
{
    var nearest_enemy
    nearest_enemy = instance_nearest(x, y, obj_parent_shot)
    if (nearest_enemy.enemies == false)
    {
        path = path_end();
        test = false;
        direction = point_direction(x, y, nearest_enemy.x, nearest_enemy.y) + 180;
        bounce = 1;
        hp -= nearest_enemy.damage
        if (image_blend != c_red)
            image_blend = c_red;
        with (nearest_enemy)
        {
            instance_destroy()
            instance_create(x, y, obj_end_shot);
            audio_play_sound(Fap, 10, false);
        }
    }
}
if (bounce == 1)
{
    alarm[0] = 10;
    speed = 2
    bounce = 0;
}

if (hp &lt;= 250)
{
    image_speed = 0.50
    speed = 3
}

if (hp &lt;= 0)
{
    instance_destroy()
    instance_create(x, y, obj_pacman_end);
    if (Player_obj.Nfloor == 0)
        instance_create(view_xview[0] + (view_wview[0] / 2), view_yview[0] + 300, obj_change_room);
}
